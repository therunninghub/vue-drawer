{
  "name": "@therunninghub/vue-drawer",
  "version": "1.0.6",
  "author": "The Running Hub Team &lt;engineer@therunninghub.net&gt;",
  "contributors": [
    {
      "name": "Le Minh Tri",
      "email": "tri@therunninghub.net"
    }
  ],
  "main": "dist/vue-drawer.umd.min.js",
  "private": false,
  "description": "A tiny Drawer component for Vue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:docs",
    "build:lib": "vue-cli-service build --target lib --name vue-drawer src/index.js",
    "build:docs": "vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "report": "vue-cli-service build --report",
    "bundle": "npm run lint --fix && npm run test:unit && npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "core-js": "^3.9.1",
    "v-animate-css": "^0.0.3",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.1.3",
    "babel-eslint": "^10.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/therunninghub/vue-drawer.git"
  },
  "bugs": {
    "url": "https://github.com/therunninghub/vue-drawer/issues"
  },
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*",
    "package.json",
    "*.js",
    "README.md"
  ],
  "keywords": [
    "vue",
    "drawer",
    "component",
    "panel",
    "layout"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
